  },
  "dependencies": {
    "@acme/tailwind-config": "*",
    "@manypkg/cli": "^0.19.2",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "turbo": "^1.5.5"
  }
}
